<view class="page"> 
    <view class="container"> 
       <view class="getmoney">
       <form bindsubmit='formsubmit'> 
         <view class="remain">
           <text class="remaincolumn">账户余额</text>
           <view class="remainright">
             <text class="remainsum">{{remain}}</text>
             <text class="remainyuan">元</text>
           </view>
         </view> 
        <view class="space">
          <view class="hint" hidden="{{hidden}}">余额不足</view>
        </view>
         <view class="withdraw">
           <text class="withdrawcolumn">提现金额</text>
           <view class="withdrawunder">
             <text class="withdrawyuan">￥</text>
             <input class="withdrawsum" name="digit" type="digit" bindinput="checksum" value="{{withdraw}}"/>
           </view>
           <view class="withdrawbelow"></view>
           <text class="submitall" bindtap="allsubmit">全部提现</text>
         </view>
         
         <view>
           <button class="submitbutton" type="primary" form-type='submit' bindtap="checksubmit" disabled="{{flag}}">提现</button>
         </view>
         </form>
       </view>
    </view>
</view>