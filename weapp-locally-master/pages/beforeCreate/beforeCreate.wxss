 .cells {
  margin-top: 20rpx;
}

.cells .item {
  height:60rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #f0f0f0;
  background-color: #fff;
  font-size: 30rpx;
}

