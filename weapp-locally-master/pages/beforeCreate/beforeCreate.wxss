.cells .divider {
  height: 20rpx;
  box-shadow: inset 0 1rpx 2rpx #eee;
}
.item .need{
  /* background-color: yellow; */
  border:1px solid #ddd;
  border-radius: 10rpx;
  display: inline-block;
    vertical-align:middle;

  height:50rpx;
  width:70rpx;
  margin:5rpx 5rpx 0rpx;
  padding:5rpx 20rpx;
}

.inputwenjuan{
  display: flex;
  justify-content: space-between;
  position: relative;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #f0f0f0;
  background-color: #fff;
  font-size: 30rpx;
  flex-direction: column;
}
.inputwenjuan .smallinput{
  margin:20rpx 0;
}
.inputwenjuan input{
  border:1px solid #ddd;
  border-radius: 10rpx;
  height:60rpx;
  width:90%;
  padding:10rpx 20rpx;
}
.inputwenjuan textarea{
  border:1px solid #ddd;
  border-radius: 10rpx;
  /* height:60rpx; */
  width:90%;
  padding:10rpx 20rpx;
}
.hiddenitem{
  background-color: #f7f7f7;
  display: flex;
  flex-direction: column;
}

.picker{
  padding: 19rpx 26rpx;
  background-color: #f7f7f7;
}
.weui-label{
  font-size:30rpx;
  /* background-color: red; */
}
.grey{
  border-color:#ddd;
  border-width:0 1rpx 1rpx;
  border-style: solid;
  padding:10rpx 0;
  height:60rpx;
  line-height:60rpx
}
.weui-cell__hd{
  height:60rpx;
  line-height:60rpx
}
.weui-cell__bd{
  height:60rpx;
  line-height:60rpx;
  display: flex;
  flex-direction: row;
}
.weui-label{
  display: flex;
  flex-direction: row;
}
.weui-input{
    height:60rpx;
  line-height:60rpx;
}
.redbaginput{
  text-align:right;
  padding:0 10rpx;
}
.new{
  margin:80rpx 0;
  background-color: #3a4861;
  color:white;
  width:100%;
  text-align:center;
  height:80rpx;
  line-height:60rpx;
  padding:10rpx 0;
  border:1px solid #3a4861;
}


